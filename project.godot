config_version=5

[application]

config/name="The Legend of Link"
run/main_scene="res://scenes/levels/area01/00_village.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")

[autoload]

Music="*res://scenes/music background/music.tscn"
PlayerHud="*res://scenes/GUI/player HUD/player_hud.tscn"
GlobalPlayerManager="*res://scenes/00_globals/global_player_manager.gd"
GlobalLevelManager="*res://scenes/00_globals/global_level_manager.gd"
SceneTransition="*res://scenes/GUI/scene transition/scene_transition.tscn"
GlobalSaveManager="*res://scenes/00_globals/global_save_manager.gd"
PauseMenu="*res://scenes/GUI/pause menu/pause_menu.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

